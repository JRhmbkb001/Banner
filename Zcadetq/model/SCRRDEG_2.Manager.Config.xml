<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="SCRRDEG_2.Manager.Config" name="SCRRDEG" targetTableName="SCRRDEG" keyMode="Unique" enforceUnique="true" >
		
		<items>
			<item name="SCRRDEG_DEGC_CODE" logicalName="scrrdegDegcCode" type="String" physicalName="SCRRDEG_DEGC_CODE" key="true"/> 
			<item name="DEGC_DESCRIPTION" logicalName="degcDescription" type="String" virtual="true"/> 
			<item name="SCRRDEG_SUBJ_CODE" logicalName="scrrdegSubjCode" type="String" physicalName="SCRRDEG_SUBJ_CODE" key="true"/> 
			<item name="SCRRDEG_CRSE_NUMB" logicalName="scrrdegCrseNumb" type="String" physicalName="SCRRDEG_CRSE_NUMB" key="true"/> 
			<item name="SCRRDEG_TERM_CODE_EFFECTIVE" logicalName="scrrdegTermCodeEffective" type="String" physicalName="SCRRDEG_TERM_CODE_EFFECTIVE" key="true"/> 
		</items>
		
    <managerClass>morphis.foundations.flavors.forms.appsupportlib.model.DBBusinessObject</managerClass>
		<adapterClass>net.hedtech.ucdavis.ps.banner.student.Zcadetq.model.Scrrdeg2Adapter</adapterClass>
		<defaultWhereClause><![CDATA[WHERE scrrdeg_rec_type = '2' AND scrrdeg_subj_code = :KEY_BLOCK_SCACRSE_SUBJ_CODE AND scrrdeg_crse_numb = :KEY_BLOCK_SCACRSE_CRSE_NUMB AND scrrdeg_term_code_effective = (SELECT max(b.scrrdeg_term_code_effective)  FROM scrrdeg b  WHERE b.scrrdeg_subj_code = :KEY_BLOCK_SCACRSE_SUBJ_CODE AND b.scrrdeg_crse_numb = :KEY_BLOCK_SCACRSE_CRSE_NUMB AND b.scrrdeg_term_code_effective <= :KEY_BLOCK_STVTERM_CODE AND b.scrrdeg_rec_type = '2' )]]></defaultWhereClause>	
		<defaultOrderByClause><![CDATA[order by scrrdeg_degc_code]]></defaultOrderByClause>	

	</manager>
</b:beans>
