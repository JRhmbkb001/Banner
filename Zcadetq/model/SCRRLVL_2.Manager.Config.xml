<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="SCRRLVL_2.Manager.Config" name="SCRRLVL" targetTableName="SCRRLVL" keyMode="Unique" >
		
		<items>
			<item name="SCRRLVL_EFF_TERM" logicalName="scrrlvlEffTerm" type="String" physicalName="SCRRLVL_EFF_TERM" key="true"> 
				<copyValueFrom>
					<item name="STVTERM_CODE" businessObject="KEY_BLOCK"/>
				</copyValueFrom>
			</item>
			<item name="SCRRLVL_SUBJ_CODE" logicalName="scrrlvlSubjCode" type="String" physicalName="SCRRLVL_SUBJ_CODE" key="true"/> 
			<item name="SCRRLVL_CRSE_NUMB" logicalName="scrrlvlCrseNumb" type="String" physicalName="SCRRLVL_CRSE_NUMB" key="true"> 
				<copyValueFrom>
					<item name="SCACRSE_CRSE_NUMB" businessObject="KEY_BLOCK"/>
				</copyValueFrom>
			</item>
			<item name="SCRRLVL_LEVL_CODE" logicalName="scrrlvlLevlCode" type="String" physicalName="SCRRLVL_LEVL_CODE" key="true"/> 
			<item name="STVLEVL_DESC" logicalName="stvlevlDesc" type="String" virtual="true"/> 
		</items>
		
    <managerClass>morphis.foundations.flavors.forms.appsupportlib.model.DBBusinessObject</managerClass>
		<adapterClass>net.hedtech.ucdavis.ps.banner.student.Zcadetq.model.Scrrlvl2Adapter</adapterClass>
		<defaultWhereClause><![CDATA[WHERE scrrlvl_rec_type = '2' AND scrrlvl_subj_code = :KEY_BLOCK_SCACRSE_SUBJ_CODE AND scrrlvl_crse_numb = :KEY_BLOCK_SCACRSE_CRSE_NUMB AND scrrlvl_eff_term = (SELECT max(b.scrrlvl_eff_term)  FROM scrrlvl b  WHERE b.scrrlvl_subj_code = :KEY_BLOCK_SCACRSE_SUBJ_CODE AND b.scrrlvl_crse_numb = :KEY_BLOCK_SCACRSE_CRSE_NUMB AND b.scrrlvl_eff_term <= :KEY_BLOCK_STVTERM_CODE AND b.scrrlvl_rec_type = '2' )]]></defaultWhereClause>	
		<defaultOrderByClause><![CDATA[order by scrrlvl_levl_code]]></defaultOrderByClause>	

	</manager>
</b:beans>
