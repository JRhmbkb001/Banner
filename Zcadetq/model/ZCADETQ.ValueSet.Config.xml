		<column name="SCBCRKY_CRSE_NUMB" type="String" size="5"/>
		<column name="SCBCRSE_TITLE" type="String" size="30"/>
		<column name="SCBCRKY_TERM_CODE_START" type="String" size="6"/>
		<column name="SCBCRKY_TERM_CODE_END" type="String" size="6"/>
		<column name="SCBCRKY_ACTIVITY_DATE" type="Date" size="7"/>
	</columns>
	<query><![CDATA[
	SELECT scbcrky_subj_code, scbcrky_crse_numb, scbcrse_title, scbcrky_term_code_start, scbcrky_term_code_end, scbcrky_activity_date  FROM scbcrse, scbcrky  WHERE (scbcrky_subj_code = :SCRRTST_SUBJ_CODE_PREQ OR :SCRRTST_SUBJ_CODE_PREQ IS NULL) AND scbcrky_subj_code = SCBCRSE_SUBJ_CODE AND scbcrky_crse_numb = SCBCRSE_CRSE_NUMB AND SCBCRKY_TERM_CODE_START = SCBCRSE_EFF_TERM  ORDER BY scbcrky_subj_code, scbcrky_crse_numb
	]]>
	</query>
	
    <parameters>
		<parameter name="SCRRTST_SUBJ_CODE_PREQ">
			<value>
				<item businessObject="SCRRTST" name="SCRRTST_SUBJ_CODE_PREQ"/>
			</value>
        </parameter>
	</parameters>
</valueSet>
