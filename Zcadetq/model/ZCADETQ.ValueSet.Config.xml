<valueSet id="STQTERM_RG.ValueSet.Config" name="STQTERM_RG">
    <columns>
		<column name="STVTERM_CODE" type="String" size="6"/>
		<column name="STVTERM_DESC" type="String" size="30"/>
		<column name="STVTERM_START_DATE" type="Date" size="7"/>
		<column name="STVTERM_END_DATE" type="Date" size="7"/>
		<column name="STVTERM_ACTIVITY_DATE" type="Date" size="7"/>
	</columns>
	<query><![CDATA[
	SELECT stvterm_code, stvterm_desc, stvterm_start_date, stvterm_end_date, stvterm_activity_date  FROM stvterm  WHERE stvterm_code >= (SELECT scbcrky_term_code_start  FROM scbcrky  WHERE scbcrky_subj_code = :SCACRSE_SUBJ_CODE AND scbcrky_crse_numb = :SCACRSE_CRSE_NUMB ) AND stvterm_code <= (SELECT scbcrky_term_code_end  FROM scbcrky  WHERE scbcrky_subj_code = :SCACRSE_SUBJ_CODE AND scbcrky_crse_numb = :SCACRSE_CRSE_NUMB )  ORDER BY 1
	]]>
	</query>
	
    <parameters>
		<parameter name="SCACRSE_SUBJ_CODE">
			<value>
				<item businessObject="KEY_BLOCK" name="SCACRSE_SUBJ_CODE"/>
			</value>
        </parameter>
		<parameter name="SCACRSE_CRSE_NUMB">
			<value>
				<item businessObject="KEY_BLOCK" name="SCACRSE_CRSE_NUMB"/>
			</value>
        </parameter>
	</parameters>
</valueSet>
