<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="SCRRCHR_2.Manager.Config" name="SCRRCHR" targetTableName="SCRRCHR" >
		
		<items>
			<item name="SCRRCHR_EFF_TERM" logicalName="scrrchrEffTerm" type="String" physicalName="SCRRCHR_EFF_TERM"> 
				<copyValueFrom>
					<item name="STVTERM_CODE" businessObject="KEY_BLOCK"/>
				</copyValueFrom>
			</item>
			<item name="SCRRCHR_SUBJ_CODE" logicalName="scrrchrSubjCode" type="String" physicalName="SCRRCHR_SUBJ_CODE"/> 
			<item name="SCRRCHR_CRSE_NUMB" logicalName="scrrchrCrseNumb" type="String" physicalName="SCRRCHR_CRSE_NUMB"> 
				<copyValueFrom>
					<item name="SCACRSE_CRSE_NUMB" businessObject="KEY_BLOCK"/>
				</copyValueFrom>
			</item>
			<item name="SCRRCHR_CHRT_CODE" logicalName="scrrchrChrtCode" type="String" physicalName="SCRRCHR_CHRT_CODE"/> 
			<item name="STVCHRT_DESC" logicalName="stvchrtDesc" type="String" virtual="true"/> 
		</items>
		
    <managerClass>morphis.foundations.flavors.forms.appsupportlib.model.DBBusinessObject</managerClass>
		<adapterClass>net.hedtech.ucdavis.ps.banner.student.Zcadetq.model.Scrrchr2Adapter</adapterClass>
		<defaultWhereClause><![CDATA[WHERE scrrchr_rec_type = '2' AND scrrchr_subj_code = :KEY_BLOCK_SCACRSE_SUBJ_CODE AND scrrchr_crse_numb = :KEY_BLOCK_SCACRSE_CRSE_NUMB AND scrrchr_eff_term = (SELECT max(b.scrrchr_eff_term)  FROM scrrchr b  WHERE b.scrrchr_subj_code = :KEY_BLOCK_SCACRSE_SUBJ_CODE AND b.scrrchr_crse_numb = :KEY_BLOCK_SCACRSE_CRSE_NUMB AND b.scrrchr_eff_term <= :KEY_BLOCK_STVTERM_CODE AND b.scrrchr_rec_type = '2' )]]></defaultWhereClause>	
		<defaultOrderByClause><![CDATA[order by scrrchr_chrt_code]]></defaultOrderByClause>	

	</manager>
</b:beans>
