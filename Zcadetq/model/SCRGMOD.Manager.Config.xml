<?xml version="1.0" encoding="UTF-8"?>
<b:beans 
	xmlns="urn:schemas:morphis:foundations:businessobject"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:b="http://www.springframework.org/schema/beans"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd urn:schemas:morphis:foundations:businessobject http://www.morphis-tech.com/schema/foundations/businessobject.xsd">
	<manager id="SCRGMOD.Manager.Config" name="SCRGMOD" targetTableName="SCRGMOD" keyMode="Unique" >
		
		<items>
			<item name="SCRGMOD_SUBJ_CODE" logicalName="scrgmodSubjCode" type="String" physicalName="SCRGMOD_SUBJ_CODE"> 
				<copyValueFrom>
					<item name="SCACRSE_SUBJ_CODE" businessObject="KEY_BLOCK"/>
				</copyValueFrom>
			</item>
			<item name="SCRGMOD_CRSE_NUMB" logicalName="scrgmodCrseNumb" type="String" physicalName="SCRGMOD_CRSE_NUMB"> 
				<copyValueFrom>
					<item name="SCACRSE_CRSE_NUMB" businessObject="KEY_BLOCK"/>
				</copyValueFrom>
			</item>
			<item name="SCRGMOD_EFF_TERM" logicalName="scrgmodEffTerm" type="String" physicalName="SCRGMOD_EFF_TERM"/> 
			<item name="SCRGMOD_ACTIVITY_DATE" logicalName="scrgmodActivityDate" type="java.util.Date" physicalName="SCRGMOD_ACTIVITY_DATE"/> 
			<item name="GMOD_INSERT" logicalName="gmodInsert" type="String" virtual="true"/> 
			<item name="TEMP_DEF" logicalName="tempDef" type="String" virtual="true"/> 
			<item name="SCRGMOD_GMOD_CODE" logicalName="scrgmodGmodCode" type="String" physicalName="SCRGMOD_GMOD_CODE"/> 
			<item name="STVGMOD_DESCRIPTION" logicalName="stvgmodDescription" type="String" virtual="true"/> 
		</items>
		
    <managerClass>morphis.foundations.flavors.forms.appsupportlib.model.DBBusinessObject</managerClass>
		<adapterClass>net.hedtech.ucdavis.ps.banner.student.Zcadetq.model.ScrgmodAdapter</adapterClass>
		<defaultWhereClause><![CDATA[WHERE (SCRGMOD_SUBJ_CODE= :KEY_BLOCK_SCACRSE_SUBJ_CODE) AND (SCRGMOD_CRSE_NUMB= :KEY_BLOCK_SCACRSE_CRSE_NUMB) AND scrgmod_eff_term = (SELECT max(b.scrgmod_eff_term)  FROM scrgmod b  WHERE b.scrgmod_eff_term <= :KEY_BLOCK_STVTERM_CODE AND b.scrgmod_subj_code = :KEY_BLOCK_SCACRSE_SUBJ_CODE AND b.scrgmod_crse_numb = :KEY_BLOCK_SCACRSE_CRSE_NUMB )]]></defaultWhereClause>	
		<defaultOrderByClause><![CDATA[order by scrgmod_default_ind, scrgmod_gmod_code]]></defaultOrderByClause>	

	</manager>
</b:beans>
